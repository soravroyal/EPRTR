	<xs:complexType name="QuantityType">
		<xs:annotation>
			<xs:documentation>TNE,</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="1" maxOccurs="1">
			<xs:element name="Quantity" type="QuantityContentType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="UnitCode" type="UnitCodeType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="UnitCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="A90 - gigawatt"/>
			<xs:enumeration value="GV - gigajoule"/>
			<xs:enumeration value="GWH - gigawatt hour"/>
			<xs:enumeration value="HLT - hectolitre"/>
			<xs:enumeration value="KGM - kilogram"/>
			<xs:enumeration value="KWH - kilowatt hour"/>
			<xs:enumeration value="KWT - kilowatt"/>
			<xs:enumeration value="LTR - litre"/>
			<xs:enumeration value="MAW - megawatt"/>
			<xs:enumeration value="MTK - square metre"/>
			<xs:enumeration value="MTQ - cubic metre"/>
			<xs:enumeration value="MWH - megawatt hour"/>
			<xs:enumeration value="PCS - pieces"/>
			<xs:enumeration value="TNE - tonne (metric ton)"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="AddressType">
		<xs:annotation>
			<xs:documentation>The base format for addresses in PRTR.
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StreetName" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name, expressed as text, of a street in the address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BuildingNumber" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The building number, expressed as text, in the address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="City" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The name, expressed as text, of the city, town or village in the address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PostalCode" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The postcode, expressed as text, if the address.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CountryID" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The unique identifier of a country in the structured address (Reference ISO 3166 and UN/ECE Rec 3).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="TotalEmissionsToAirType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Dust"/>
			<xs:enumeration value="NOx"/>
			<xs:enumeration value="SO2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="MethodUsedType">
		<xs:annotation>
			<xs:documentation>Describes a measurement/calculation method used for determination of quantities of pollutants and waste
            </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MethodTypeCode" type="MethodTypeCodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Identifies the type of method used (CEN/ISO, ETS, etc.). Notice that the method type “WEIGH” (i.e. weighing) only is allowed for waste transfer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Designation" type="TextType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>A short designation or description of the method used. Required for some method types as indicated in the list of legal method type codes.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="MediumCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="AIR"/>
			<xs:enumeration value="LAND"/>
			<xs:enumeration value="WATER"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>

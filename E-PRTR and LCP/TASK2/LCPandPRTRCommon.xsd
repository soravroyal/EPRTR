			<xs:enumeration value="A42h - Article 4(2)(h) of Directive 2003/4/EC"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="WasteTypeCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="HW">
				<xs:annotation>
					<xs:documentation>Hazardous waste</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HWIC">
				<xs:annotation>
					<xs:documentation>Hazardous waste within country</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HWOC">
				<xs:annotation>
					<xs:documentation>Hazardous waste outside country</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NON-HW">
				<xs:annotation>
					<xs:documentation>Non-hazardous waste</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="WasteTreatmentCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="D - Destined for disposal"/>
			<xs:enumeration value="R - Destined for recovery"/>
		</xs:restriction>
	</xs:simpleType>

<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://dd.eionet.europa.eu/schemas/LCPandPRTR/LCPandPRTRCommon" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://dd.eionet.europa.eu/schemas/LCPandPRTR/LCPandPRTRCommon" xmlns:dc="http://purl.org/dc/elements/1.1/" elementFormDefault="qualified">
	<xs:annotation>
		<xs:documentation>
				This schema defines the common types used by LCP reporting schemas. It is a reference schema only and does not have its own instance
		</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="OtherGasesType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Blast furnace gas"/>
			<xs:enumeration value="Products of gasification"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OtherSolidFuels">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Peat"/>
			<xs:enumeration value="Lignite"/>
			<xs:enumeration value="Sub-bituminous coal"/>
			<xs:enumeration value="Bituminous coal"/>
			<xs:enumeration value="Anthracite"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TextType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LongTextType">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReportingYearType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="2006"/>
			<xs:totalDigits value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QuantityContentType">
		<xs:restriction base="xs:decimal">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TrueFalseType">
		<xs:annotation>
			<xs:documentation>Checkbox values</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="true"/>
			<xs:enumeration value="false"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MethodBasisCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="C - Calculated"/>
			<xs:enumeration value="E - Estimated"/>
			<xs:enumeration value="M - Measured"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MethodTypeCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="ALT - Alternative measurement methodology in accordance with existing CEN/ISO measurements standards"/>
			<xs:enumeration value="CEN/ISO - Internationally approved measurement standard"/>
			<xs:enumeration value="CRM - Measurement methodology for the performance of which is demonstrated by means of certified reference materials and accepted by competent authority."/>
			<xs:enumeration value="ETS - Guidelines for the monitoring and reporting of greenhouse gas emissions under the Emission Trading Scheme"/>
			<xs:enumeration value="IPCC - IPCC Guidelines"/>
			<xs:enumeration value="MAB - Mass balance method which is accepted by the competent authority"/>
			<xs:enumeration value="NRB - National or regional binding measurement/calculation methodology prescribed by legal act for the pollutant and facility concerned"/>
			<xs:enumeration value="OTH - Other measurement/calculation methodology"/>
			<xs:enumeration value="PER - Measurement/Calculation Methodology already prescribed by the competent authority in a licence or an operating permit for the facility"/>
			<xs:enumeration value="SSC - European-wide sector specific calculation method"/>
			<xs:enumeration value="UNECE/EMEP - UNECE/EMEP EMEP/CORINAIR Emission Inventory Guidebook"/>
			<xs:enumeration value="WEIGH - Weighing"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatusOfThePlantType">
		<xs:annotation>
			<xs:documentation>Pollutants, Form 5 "Pollutants not covered by TNP"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Art 30(2) - existing plants under IED"/>
			<xs:enumeration value="Art 30(3) - new plants under IED"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CombustionType">
		<xs:restriction base="xs:string">

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="config_enh.xsd">
	<userinterface>
		<collapsewidgets>true</collapsewidgets>
		<hidewidgetscontrol>false</hidewidgetscontrol>
		<banner>visible</banner>
		<title>E-PRTR</title>
		<locale>en_GB</locale>
		<subtitle>The European Pollutant Release and Transfer Register</subtitle>
		<logo>com/esri/solutions/flexviewer/assets/images/icons/eprtrimagelogo.png</logo>
		<stylesheet>com/esri/solutions/flexviewer/themes/lighterside/style.swf</stylesheet>
		<menus>
			<menu id="menuMap" visible="true" icon="com/esri/solutions/flexviewer/assets/images/icons/i_globe.png">Background Map</menu>
			<menu id="menuNav" target="customMenu" visible="true" icon="com/esri/solutions/flexviewer/assets/images/icons/i_nav.png">Navigation</menu>
			<menu id="menuWidgets" visible="false" icon="com/esri/solutions/flexviewer/assets/images/icons/i_widget.png">Tools</menu>
		</menus>
	</userinterface>
	<map initialExtent="-32.2 12.4 54.59 86.92" fullExtent="-150 -50 150 50" showzoomslider="false" scrollwheelzoomenabled="true">
		<basemaps menu="menuMap">
			<mapservice label="Street Map" type="tiled" visible="true" alpha="1" icon="com/esri/solutions/flexviewer/assets/images/icons/i_highway.png" >http://server.arcgisonline.com/ArcGIS/rest/services/ESRI_StreetMap_World_2D/MapServer</mapservice>
			<mapservice label="Satellite" type="tiled" visible="false" alpha="1" icon="com/esri/solutions/flexviewer/assets/images/icons/i_satellite.png" url="http://server.arcgisonline.com/ArcGIS/rest/services/ESRI_Imagery_World_2D/MapServer"></mapservice>
			<mapservice label="River Basin Districts" type="tiled" visible="false" alpha="1" icon="com/esri/solutions/flexviewer/assets/images/icons/i_RBD_layer.png" overlay="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_RBDs/MapServer" overlaytype="dynamic" overlayalpha="0.6" legend="com/esri/solutions/flexviewer/assets/images/legends/[locale]/EPRTR_RBD_legend.png" >http://server.arcgisonline.com/ArcGIS/rest/services/ESRI_Imagery_World_2D/MapServer</mapservice>
		</basemaps>
		<livemaps>
			<mapservice label="9 Other activities" id="sector9" iconShape="triangle" iconSize="10" iconColor="0xFF009D" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector9/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '9' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice id="sector8" label="8 Animal and vegetable products from the food and beverage sector" iconShape="triangle" iconSize="10" iconColor="0xE6E600" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector8/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '8' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice id="sector7" label="7 Intensive livestock production and aquaculture" iconShape="triangle" iconSize="10" iconColor="0xA80000" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector7/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '7' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice id="sector6" label="6 Paper and wood production processing" iconShape="triangle" iconSize="10" iconColor="0x8400A8" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector6/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '6' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice id="sector5" label="5 Waste and waste water management" iconShape="triangle" iconSize="10" iconColor="0x55FF00" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector5/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '5' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice id="sector4" label="4 Chemical industry" iconShape="triangle" iconSize="10" iconColor="0x000000" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector4/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '4' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice  id="sector3" label="3 Mineral industry" iconShape="triangle" iconSize="10" iconColor="0xFF5500" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector3/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '3' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>
			<mapservice id="sector2" label="2 Production and processing of metals " iconShape="triangle" iconSize="10" iconColor="0x00E6A9" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector2/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '2' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>			
			<mapservice id="sector1" label="1 Energy sector" iconShape="triangle" iconSize="10" iconColor="0x267300" type="dynamic" visible="false" alpha="1" url="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB/EPRTR_Sector1/MapServer">
				<layers>
						<layer id="0" visible="true" definition="IASectorCode = '1' AND CoordinateStatusCode &lt;&gt; 'MISSING' [wherevalue]"></layer>
				</layers>
			</mapservice>		
		</livemaps>
	</map>
	<navtools>
		<navtool label="Zoom In" icon="com/esri/solutions/flexviewer/assets/images/icons/i_zoomin.png" menu="menuNav">zoomin</navtool>
		<navtool label="Zoom Out" icon="com/esri/solutions/flexviewer/assets/images/icons/i_zoomout.png" menu="menuNav">zoomout</navtool>
		<navtool label="Pan" icon="com/esri/solutions/flexviewer/assets/images/icons/i_pan.png" menu="menuNav">pan</navtool>
		<navtool label="Identify" icon="com/esri/solutions/flexviewer/assets/images/icons/i_info_bubble.png" menu="menuNav">pointsearch</navtool>
	</navtools>
	<widgets>
		<widget label="Overview Map" preload="true"  widgetresourcefile="OverviewMapWidgetStrings" invisbtns="close" icon="com/esri/solutions/flexviewer/assets/images/icons/i_overview.png" menu="menuWidgets" config="com/esri/solutions/flexviewer/widgets/OverviewMapWidget.xml">com/esri/solutions/flexviewer/widgets/OverviewMapWidget.swf</widget>	
		<widget label="Bookmarks" preload="true" widgetresourcefile="BookmarkWidgetStrings"  invisbtns="close" icon="com/esri/solutions/flexviewer/assets/images/icons/i_bookmark.png"  config="com/esri/solutions/flexviewer/widgets/BookmarkWidget.xml">com/esri/solutions/flexviewer/widgets/BookmarkWidget.swf</widget>	
		<widget label="Layer List" invisbtns="close"  widgetresourcefile="FilterAllWidgetStrings" preload="true" icon="com/esri/solutions/flexviewer/assets/images/icons/i_layers.png"  menu="menuWidgets"  config="com/esri/solutions/flexviewer/widgets/FilterLiveMapsWidget.xml">com/esri/solutions/flexviewer/widgets/FilterLiveMapsWidget.swf</widget>
		<widget label="Search" widgetresourcefile="SearchAllWidgetStrings" invisbtns="close" preload="true" icon="com/esri/solutions/flexviewer/assets/images/icons/i_search.png" menu="menuWidgets" config="com/esri/solutions/flexviewer/widgets/SearchWidget.xml" baserestserverurl="http://sdkcga6307/ArcGIS/rest/services/PRTR_WEB">com/esri/solutions/flexviewer/widgets/SearchWidgetAllLayers.swf</widget>	
		<widget label="Print and Download"  widgetresourcefile="PrintWidgetStrings" icon="com/esri/solutions/flexviewer/assets/images/icons/i_print_download.png" menu="menuWidgets" invisbtns="close" preload="true" config="com/esri/solutions/flexviewer/widgets/PrintWidget.xml">com/esri/solutions/flexviewer/widgets/PrintWidget.swf</widget>		
		<widget label="ga"  preload="true" icon="com/esri/solutions/flexviewer/assets/images/icons/i_search.png" menu="menuHelp" config="com/esri/solutions/flexviewer/widgets/GoogleAnalyticsWidget.xml" >com/esri/solutions/flexviewer/widgets/GoogleAnalyticsWidget.swf</widget>
	</widgets>
	<links>
			</links>
	<proxytype>php</proxytype>
	<!-- apache|jsp|asp|php -->
</configuration>
